      <Button fx:id="newProfileButton" mnemonicParsing="false" onAction="#createProfile" text="Create New Profile" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <ComboBox fx:id="userComboBox" prefWidth="150.0" promptText="Select Profile" GridPane.rowIndex="1" />
